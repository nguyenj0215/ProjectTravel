<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.projecttravel.projecttravel">

<application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity android:name=".MainActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>

    <activity
        android:name=".Main2Activity"
        android:parentActivityName="com.projecttravel.projecttravel.MainActivity">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.projecttravel.projecttravel.MainActivity"/>
        <intent-filter>
            <action android:name="com.projecttravel.projecttravel.Main2Activity" />
            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>
    </activity>

    <activity
        android:name=".Main3Activity"
        android:parentActivityName="com.projecttravel.projecttravel.MainActivity">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.projecttravel.projecttravel.MainActivity"/>
        <intent-filter>
            <action android:name="com.projecttravel.projecttravel.Main3Activity" />
            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>
    </activity>

    <activity
        android:name=".Main4Activity"
        android:parentActivityName="com.projecttravel.projecttravel.MainActivity">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.projecttravel.projecttravel.MainActivity"/>
        <intent-filter>
            <action android:name="com.projecttravel.projecttravel.Main4Activity" />
            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>
    </activity>

</application>
</manifest>